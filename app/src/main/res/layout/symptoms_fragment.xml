<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.main.fragments.SymptomsFragment">

    <include
        android:id="@+id/custom_tab"
        layout="@layout/custom_tab_layout"/>

    <ImageView
        android:id="@+id/health_in_process_img"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:src="@drawable/ic_undraw_completed_steps_re_h9wc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/pf_text_view_not_registred_txt"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="В работе..."
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/main_blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/health_in_process_img" />

</androidx.constraintlayout.widget.ConstraintLayout>
